<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Your Peerfolio</title>
  </head>
  <body>
    <%- include('../partials/nav.ejs') %>
    <h3>yearbook posts</h3>

    <% yearbookPosts.forEach((post) => { %> <% if (post.post_id) { %>
    <img
      src="<%=post.post_id.image.replace(/^public/, '')%>"
      alt="Post Image"
      style="width: 200px"
    />
    <p><%= post.quote%></p>
    <p><%= post.year%></p>

    <% }}) %>
  </body>
</html>
